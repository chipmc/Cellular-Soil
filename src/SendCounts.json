{
  "event": "Cellular_Soil_Hook",
  "url": "https://things.ubidots.com/api/v1.6/devices/{{PARTICLE_DEVICE_ID}}/?token={{Put your token here}}",
  "requestType": "POST",
  "mydevices": true,
  "noDefaults": true,
  "responseTemplate": "{{temperature.0.status_code}}",
  "responseTopic": "{{PARTICLE_DEVICE_ID}}_Send_Counts",
  "json": {
    "Temperature": "{{Temperature}}",
    "Humidity": "{{Humidity}}",
    "SoilConductivity": "{{SoilConductivity}}",
    "SoilTemp": "{{SoilTemp}}",
    "SoilMoisture": "{{SoilMoisture}}",
    "Battery": "{{Battery}}",
    "Resets": "{{Resets}}",
    "Alerts": "{{Alerts}}"
  }
}
